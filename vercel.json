{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node",
      "config": {
        "maxDuration": 120
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/index.js"
    }
  ],
  "ignore": [
    "api/index-broken.js",
    "api/index-mock.js",
    "api/try-on.js",
    "api/webhooks.js",
    "api/controllers/**",
    "api/services/**",
    "src/**",
    "*.test.js",
    "test-*.js"
  ]
}