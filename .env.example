# Server Configuration
NODE_ENV=production
PORT=3000

# Supabase Configuration
SUPABASE_URL=your_supabase_project_url
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key
SUPABASE_ANON_KEY=your_supabase_anon_key

# Redis Configuration (untuk production, gunakan Redis cloud)
REDIS_HOST=your_redis_host
REDIS_PORT=6379
REDIS_PASSWORD=your_redis_password

# Pixazo AI Service Configuration
PIXAZO_API_URL=https://gateway.pixazo.ai/virtual-tryon/v1/r-vton
PIXAZO_API_KEY=your_pixazo_api_key

# Webhook Configuration (untuk Vercel deployment)
BASE_URL=https://vton.ai-agentic.tech
PIXAZO_WEBHOOK_SECRET=your_webhook_secret_here

# JWT Configuration
JWT_SECRET=your_supabase_jwt_secret
JWT_ALGORITHM=HS256

# File Upload Configuration
MAX_FILE_SIZE=10485760
ALLOWED_MIME_TYPES=image/jpeg,image/jpg,image/png

# Rate Limiting Configuration
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# CORS Configuration
ALLOWED_ORIGINS=https://lovable.ai,https://vton.ai-agentic.tech